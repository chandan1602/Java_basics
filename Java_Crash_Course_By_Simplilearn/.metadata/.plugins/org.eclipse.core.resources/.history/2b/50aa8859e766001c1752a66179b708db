package learningpart2;

import java.awt.List;
import java.util.ArrayList;

/*
 * 
 */




public class Generics {
	public static void main(String[] args) {
		//Without Generics
		List list = new ArrayList();
		list.add("hello");
		String s = (String) list.get(0);
		System.out.println(s);
	}
}
