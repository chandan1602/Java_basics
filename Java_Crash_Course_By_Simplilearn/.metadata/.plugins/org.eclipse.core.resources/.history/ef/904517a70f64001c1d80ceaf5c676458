package learning;

class A {
	public void show() {
		System.out.println("A");
	}
}

class B extends A {
	@Override
	public void show() {
		System.out.println("B");
	}
}

public class Annotations {
	public static void main(String args[]) {
		A obj = new A();
		obj.show();
		B obj2 = new B();
		obj2.show(); //OverRiding by B
	}
}
